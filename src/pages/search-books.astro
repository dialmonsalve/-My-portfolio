---
import Layout from '../layouts/Layout.astro';

import '../pages/styles/search-books.css'
---
<Layout title="Api books">

		<div>
			<h1>books</h1>
		</div>
		
		<div class="container-form" id="search-form">			
				<div class="form">

					<astro-greet>
						<form id="form-search" name="form" >
							<div class="form-control">
								
								<input type="text" name="search" id="search"/>
								<input type="submit" value="search" >
							</div>
						</form>
					</astro-greet>

				</div>
		</div>
		
		<div class="grid-card" id="container-books">
		
		</div>

</Layout>

<script is:inline src="../../src/pages/js/search-books.js"></script>




